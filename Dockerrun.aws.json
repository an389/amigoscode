{
  "AWSEBDockerrunVersion": 3,
  "containerDefinitions": [
    {
      "name": "drei-api",
      "image": "andumihai2000/drei-api",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "test"
        }
      ]
    }
  ]
}
